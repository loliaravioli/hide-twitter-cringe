{
    "manifest_version": 3,
    "name": "Twitter Hide Cringe",
    "version": "1.0",
    "description": "Hide replies and posts from people who are cringe",
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.x.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}